{
  "name": "switchmap",
  "version": "2.0.0",
  "private": true,
  "description": "SwitchMap web app",
  "author": "ultram4rine",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --single",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "mkdir -p public/build && sass --no-source-map -I node_modules src/_Typography.scss public/build/typography.css && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "rollup": "^2.66.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@felte/validator-zod": "^0.3.8",
    "@material/typography": "^13.0.0",
    "@smui/button": "^6.0.0-beta.13",
    "@smui/card": "^6.0.0-beta.13",
    "@smui/checkbox": "^6.0.0-beta.13",
    "@smui/common": "^6.0.0-beta.13",
    "@smui/drawer": "^6.0.0-beta.13",
    "@smui/form-field": "^6.0.0-beta.13",
    "@smui/icon-button": "^6.0.0-beta.13",
    "@smui/layout-grid": "^6.0.0-beta.13",
    "@smui/textfield": "^6.0.0-beta.13",
    "@smui/top-app-bar": "^6.0.0-beta.13",
    "axios": "^0.25.0",
    "felte": "^0.9.0",
    "sirv-cli": "^2.0.2",
    "svelte-material-ui": "^6.0.0-beta.13",
    "svelte-router-spa": "^6.0.3",
    "zod": "^3.11.6"
  }
}
